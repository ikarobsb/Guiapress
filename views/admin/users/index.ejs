<%- include('../../partials/header.ejs'); %>
<%- include('../../partials/navbar.ejs'); %>

<div class="container">
    <hr />
    <h2>Usuários</h2>
   <a class="btn btn-primary" href="/admin/users/create">Cadastrar novo usuário</a>
    <hr />
    <table class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th>id</th>
          <th>e-mail</th>
         
        </tr>
      </thead>
      <tbody>
        <% users.forEach(user => { %>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.email %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>

<%- include('../../partials/footer.ejs'); %>